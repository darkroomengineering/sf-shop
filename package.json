{
  "name": "satus",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "prepare": "husky install",
    "size": "npm run build && size-limit"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.16.2",
    "@radix-ui/react-accordion": "^1.1.1",
    "@studio-freight/compono": "^0.0.11",
    "@studio-freight/hamo": "^0.6.12",
    "@studio-freight/lenis": "^0.2.28",
    "@studio-freight/react-lenis": "0.0.12",
    "@studio-freight/tempus": "0.0.36",
    "blaze-slider": "1.9.0",
    "@theatre/core": "^0.6.0",
    "@theatre/studio": "^0.6.0",
    "clsx": "^1.2.1",
    "crypto-js": "^4.1.1",
    "events": "^3.3.0",
    "gsap": "^3.11.4",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "graphql-tag": "^2.12.6",
    "idb-keyval": "^6.2.0",
    "jsonminify": "^0.4.2",
    "lint-staged": "^13.2.0",
    "next": "13.5.0",
    "next-pwa": "5.6.0",
    "next-sitemap": "^4.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "^17.4.0",
    "react-use-measure": "^2.1.1",
    "sass": "^1.58.3",
    "swr": "^2.1.5",
    "throttle-debounce": "5.0.0",
    "zustand": "4.3.6"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.7.5",
    "@next/bundle-analyzer": "^13.2.4",
    "@size-limit/preset-app": "^8.2.4",
    "@svgr/webpack": "^6.5.1",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "size-limit": "^8.2.4",
    "stats.js": "^0.17.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,scss,md}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "react-is": "18.2.0",
      "throttle-debounce": "5.0.0"
    }
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/**/*.js"
    }
  ]
}
